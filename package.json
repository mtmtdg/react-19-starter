{
  "name": "react-19-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx --yes only-allow pnpm",
    "prepare": "husky",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:clean": "rm -rf dist",
    "typecheck": "tsc -b --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typos": "which typos > /dev/null && typos || true",
    "typos:fix": "which typos > /dev/null && typos --write-changes || true",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,jsonc}": [
      "pnpm run typos:fix",
      "biome check --write --no-errors-on-unmatched"
    ],
    "*.{css,scss,md,yml,yaml}": [
      "pnpm run typos:fix"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.11.0",
    "dayjs": "^1.11.18",
    "jotai": "^2.13.1",
    "jwt-decode": "^4.0.0",
    "ky": "^1.9.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "sass": "^1.91.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
